{
    "name": "shader2k/search-indexer",
    "description": "zero downtime reindex",
    "type": "library",
    "keywords": [
        "laravel",
        "elasticsearch",
        "reindex"
    ],
    "authors": [
        {
            "name": "shader2k",
            "email": "shader2k@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "extra": {
    },
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "elasticsearch/elasticsearch": "^7.0",
        "vlucas/phpdotenv": "^4.0",
        "hassankhan/config": "^2.1",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "files": [
            "src/Helpers/helpers.php"
        ],
        "psr-4": {
            "Shader2k\\SearchIndexer\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [
            "src/Helpers/helpers.php"
        ],
        "psr-4": {
            "Shader2k\\SearchIndexer\\Tests\\": "tests/"
        }
    }
}
